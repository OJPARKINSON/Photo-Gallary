<!DOCTYPE html>
<html>
    <head>
        <title>Photos</title>
        <link rel="stylesheet" href="/style.css">
    </head>
    <body>
        <div class="Header HeaderScrolled"> 
          <div class="HeaderGroup">
                <a href="/"><h1>Photo Gallery</h1></a>
                <a class="Photos" href="/photo?id=1">Photo</a>
                <a class="Photographer" href="/photographer">Photographer</a>
            </div>
        </div>
        <div class="photographer">
            <%@db.execute('SELECT id, longUrl, user_id FROM photos;') do |id, url, user_id| %>
                <p><%=user_id%></p><img src="<%=url%>">
            <%end %>
        </div>
    </body>
</html>