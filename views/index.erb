<!DOCTYPE html>

<html>
    <head>
        <link rel="stylesheet" href="/style.css">
    </head>
    <body>
        <div class="topnav">
            <a class="active" href="">Photo Gallery</a>
            <a href="/photo?id=1">Photo</a>
            <a href="">Photographer</a>
        </div>
        <div class="photoGallery"> 
            <%@db.execute('SELECT id, longUrl, likes FROM photos;') do |id, url, likes| %>
                <div class="image">
                <a href="photo?id=<%=id%>"><img src="<%=url%>"></a>
                <p><%=likes%></p>
            </div>
            <%end%>
         </div>
    </body>
</html>
